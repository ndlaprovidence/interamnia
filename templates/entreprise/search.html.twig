{% block title %}{% endblock %}

{% block body %}
<div class="jumbotron">
    <div class="container">
        {{ form_start(formSearch) }}
        <div class="form-row">
            <div class="col">
                {{ form_row(formSearch.nomEntreprise) }}
            </div>
            <div class="col">
                {{ form_row(formSearch.departementEntreprise) }}
            </div>
            <div class="col">
                {{ form_row(formSearch.villeEntreprise) }}
            </div>
            <div class="col">
                <button class="btn btn-primary">Rechercher</button>
            </div>
        </div>
        {{ form_end(formSearch) }}
    </div>
</div>
{% endblock %}